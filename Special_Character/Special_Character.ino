// Wire - Version: Latest #include <Wire.h>// LiquidCrystal I2C - Version: Latest #include <LiquidCrystal_I2C.h>/*Make a Special Character*/LiquidCrystal_I2C lcd(0x27,16,2); // set up lcdbyte smile[8] = {B00000,B10001,B00000,B00000, // Smiley FaceB10001,B01110,B00000};void setup() {  lcd.createChar(0,smile); // Set special char0 to myCharacter array   lcd.init();  lcd.backlight();  lcd.write(0); // write doesnt convert to text before printing  delay(1000);  lcd.setCursor(0,1);  lcd.print("Smile!");    }void loop() {    }