#include <Servo.h>/*Intro To Servos*/Servo myServo; // create servo object called myServoint potPin = A1;int x = 0;void setup() {    myServo.attach(3); // attach servo object to pin    myServo.write(0); // 0 to 180    delay(1000);    myServo.write(180);    pinMode(potPin, INPUT);    Serial.begin(9600);}void loop() {  //x = constrain(analogRead(potPin), 0, 180);  x = map(analogRead(potPin), 0, 1023, 0, 180);  Serial.println(x);  myServo.write(x);}